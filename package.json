{
  "name": "random_webapp",
  "version": "0.0.0",
  "description": "My website",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --watch webpack.common.js --watch webpack.dev.js --exec 'webpack-dev-server --config webpack.dev.js'",
    "build": "webpack --config webpack.prod.js",
    "webpack:dev_server": "webpack-dev-server --config webpack.dev.js",
    "webpack:build:dev": "webpack --config webpack.dev.js",
    "webpack:build:prod": "webpack --config webpack.prod.js",
    "githubio": "pnpm run build; push-dir --dir dist --branch master --remote githubio --force --cleanup",
    "pretty": "prettier --ignore-path './.gitignore' --write -c './**/*.{ts,js,vue,yaml,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx prettier --ignore-path './.gitignore' --write -c './**/*.{ts,js,vue,yaml,json}'"
    }
  },
  "author": "Danil Kireev",
  "license": "MIT",
